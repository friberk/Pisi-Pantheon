        <Summary>A modern programming language for GNOME</Summary>
        <Description>Vala is a new programming language that aims to bring modern programming language features to GNOME developers without imposing any additional runtime requirements and without using a different ABI compared to applications and libraries written in C.</Description>
        <BuildDependencies>
            <Dependency>dbus-devel</Dependency>
            <Dependency>glib2-devel</Dependency>
            <Dependency>gobject-introspection-devel</Dependency>
            <Dependency>libxslt-devel</Dependency>
        </BuildDependencies>
        <Archive sha1sum="560af22b18a1ff24076b5aae3476df0657cdbc1b" type="tarxz">mirrors://gnome/vala/0.52/vala-0.52.0.tar.xz</Archive>
    </Source>
    <Package>
        <Name>vala</Name>
        <Files>
            <Path fileType="executable">/usr/bin</Path>
            <Path fileType="library">/usr/lib</Path>
            <Path fileType="executable">/usr/lib/vala</Path>
            <Path fileType="doc">/usr/share/doc</Path>
            <Path fileType="man">/usr/share/man</Path>
            <Path fileType="data">/usr/share/vala-*/vapi</Path>
            <Path fileType="data">/usr/share/vala/*</Path>
            <Path fileType="data">/usr/share/vala/vapi/libvala-*.vapi</Path>
            <Path fileType="data">/usr/share/vim/vimfiles</Path>
        </Files>
        <AdditionalFiles>
            <AdditionalFile target="/usr/share/vim/vimfiles/ftdetect/vala.vim" permission="0644" owner="root">vim/vala.ftdetect</AdditionalFile>
            <AdditionalFile target="/usr/share/vim/vimfiles/syntax/vala.vim" permission="0644" owner="root">vim/vala.syntax</AdditionalFile>
        </AdditionalFiles>
        <Summary>A super lightweight, beautiful, and simple terminal. Comes with sane defaults, browser-class tabs, sudo paste protection, smart copy/paste, and little to no configuration.</Summary>
        <RuntimeDependencies>
            <Dependency>glib2</Dependency>
        </RuntimeDependencies>
    </Package>
    <Package>
        <Name>vala-docs</Name>
        <Files>
            <Path fileType="doc">/usr/share/devhelp</Path>
        </Files>
        <AdditionalFiles>
            <AdditionalFile target="/usr/share/vala/vala.ftdetect" group="root" permission="0644" owner="root">vim/vala.ftdetect</AdditionalFile>
            <AdditionalFile target="/usr/share/vala/vala.syntax" group="root" permission="0644" owner="root">vim/vala.syntax</AdditionalFile>
        </AdditionalFiles>
        <Summary>GNOME devhelp documentation for Vala</Summary>
    </Package>
    <Package>
        <Name>vala-devel</Name>
        <Files>
            <Path fileType="header">/usr/include</Path>
            <Path fileType="data">/usr/lib/pkgconfig</Path>
            <Path fileType="data">/usr/share/aclocal</Path>
            <Path fileType="man">/usr/share/man/man3</Path>
            <Path fileType="data">/usr/share/pkgconfig</Path>
            <Path fileType="data">/usr/share/vala/Makefile.vapigen</Path>
        </Files>
        <AdditionalFiles>
            <AdditionalFile target="/usr/share/vala/vala.ftdetect" group="root" permission="0644" owner="root">vim/vala.ftdetect</AdditionalFile>
            <AdditionalFile target="/usr/share/vala/vala.syntax" group="root" permission="0644" owner="root">vim/vala.syntax</AdditionalFile>
        </AdditionalFiles>
        <Summary>Development files for vala</Summary>
        <RuntimeDependencies>
            <Dependency>glib2-devel</Dependency>
            <Dependency release="current">vala</Dependency>
        </RuntimeDependencies>
    </Package>
